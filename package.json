{
  "name": "campjs23",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "nodemon node_modules/thalia/server/thalia.js",
    "build": "npm-run-all --parallel build:*",
    "develop": "npm-run-all --parallel develop:*",
    "build:webpack": "webpack --mode production",
    "build:config": "tsc --project config",
    "build:sass": "sass src:public",
    "develop:server": "npm start",
    "develop:sync": "sleep 1 && browser-sync start --config bs-config.js",
    "develop:sass": "sass --watch src:public",
    "develop:config": "tsc --project config --preserveWatchOutput --watch",
    "develop:models": "tsc --project models --preserveWatchOutput --watch",
    "develop:webpack": "webpack --watch --mode development"
  },
  "dependencies": {
    "@dimforge/rapier2d": "^0.11.2",
    "browser-sync": "^2.29.3",
    "d3": "^7.8.5",
    "d3-selection-multi": "^1.0.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5",
    "thalia": "github:david-ma/Thalia",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
